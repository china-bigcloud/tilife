{
    "title": "童爱生活",
    "description": "纯运维技术wiki文档",
    "author": "仝晓达",
    "language": "zh-hans",
    "gitbook": "3.2.3",
    "styles": {
        "website": "styles/website.css",
        "ebook": "styles/ebook.css",
        "pdf": "styles/pdf.css",
        "mobi": "styles/mobi.css",
        "epub": "styles/epub.css"
    },
    "output": null,
    "pdf": {
        "fontSize": 12,
        "footerTemplate": null,
        "headerTemplate": null,
        "margin": {
            "bottom": 36,
            "left": 62,
            "right": 62,
            "top": 36
        },
        "pageNumbers": true,
        "paperSize": "a4"
    },
    "plugins": [
        "-lunr",
        "-search",
        "search-pro",
        "-sharing",
        "sharing-plus",
        "-default-theme",
        "highlight",
        "back-to-top-button",
        "expandable-chapters",
        "code",
        "splitter",
        "accordion",
        "tbfed-pagefooter",
        "pageview-count",
        "chapter-fold",
        "hide-element",
        "icp",
        "donate"
    ],
    "pluginsConfig": {
        "sharing": {
            "douban": false,
             "facebook": false,
             "google": false,
             "pocket": false,
             "qq": false,
             "qzone": false,
             "twitter": false,
             "weibo": false,
             "all": ["qq", "qzone","twitter", "weibo" ]
        },
        "tbfed-pagefooter": {
            "copyright": "",
            "modify_label": "文件修改时间:",
            "modify_format": "YYYY-MM-DD HH:mm:ss"
        },
        "hide-element": {
            "elements": [". gitbook-link"]
        },
        "icp": {
            "number": "京ICP备12345678号",
            "link": "https://beian.miit.gov.cn/"
        },
        "donate": {
            "wechat": "donate-wechatpay.png",
            "alipay": "donate-alipay.png",
            "title": "",
            "button": "赏",
            "alipayText": "支付宝打赏",
            "wechatText": "微信打赏"
        }
    }
}
